--Basic  Quries

SELECT student_name, major
FROM student;

SELECT student.student_name
FROM student
ORDER BY student_name;

SELECT *
FROM student
ORDER BY student_name, major;

SELECT *
FROM student
ORDER BY student_id DESC;

SELECT student_name, major
FROM student
WHERE major = 'Bio' OR student_name = 'KRISHNA';

SELECT student_name, major
FROM student
WHERE student_id <= 3 AND student_name <> 'SANJAY';

SELECT *
FROM student
WHERE student_name IN ('KRISHNA','SANJAY');
